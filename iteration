#include<stdio.h>
int main()
{
	int n,i,a[100];
	printf("Enter the number of elements in an array:\n");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		printf("Element a[%d]",i);
		scanf("%d",&a[i]);
	}
	int j,temp,k;
	for(i=0;i<n;i++)
	{
		temp = a[i];
		for(j=i-1;j>-1;j--)
		{
			if(j>=0 && a[i]>temp)
			{
				k=a[i];
				a[i]=a[j+1];
				a[j+1]=k;
			}
		}
	}
	for(i=0;i<n;i++)
	{
		if(i==5)
		{
			printf("The 6 th element is %d",a[i]);
			
		}
	}
	return 0;
}

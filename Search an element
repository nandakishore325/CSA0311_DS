#include<stdio.h>
int main()
{
	int a[100],search,i,n;
	printf("enter the number of elements in an array:\n");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		printf("Enter the numbers in the array a[%d]:",i);
		scanf("%d",&a[i]);
	}
	printf("Enter the search element:\n");
	scanf("%d",&search);
	for(i=0;i<n;i++)
	{
		if(a[i]==search)
		{
			printf("the search element is %d and at the location %d",search,i+1);
			break;
		}
	}
	if(i==n)
	{
		printf("%d not present in the array",search);
	}
	return 0;
}
